{
  "name": "@tim-evans/rush",
  "version": "0.5.0",
  "description": "Pop over primitives",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/commonjs/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist; tsc -p . & tsc -p . --module ESNext --outDir dist/modules/ --target ES2017; exit 0",
    "test": "./node_modules/jest/bin/jest.js"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts"
    ],
    "moduleNameMapper": {
      "@tim-evans/rush": "<rootDir>/src/index.ts"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*-test.(ts|tsx)"
    ]
  }
}
